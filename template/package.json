{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prettier": "prettier --write 'src/**/*.{js,json,yml,yaml,ts,tsx,md}'",
    "prettier:gql:generated": "prettier --write 'src/graphql/operations/**/*.generated.{js,json,yml,yaml,ts,tsx,md}'",
    "commit": "npx git-cz",
    "prepare": "husky install",
    "lint-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@fruits-chain/eslint-config-rn": "^0.3.18",
    "@fruits-chain/prettier-config-preset": "^0.0.3",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@types/react": "^18",
    "prettier": "^3.1.0"
  },
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
